<h2><%= @list.name %></h2>

<ul>
<% @list.bookmarks.each do |bm| %>
  <li><%= bm.movie.title %> | <%= bm.comment %> <%= link_to "delete", bookmark_path(bm), method: :delete, data: { confirm: "Are you sure?" } %></li>
  <% end %>
</ul>

<%= link_to "add movie",  new_list_bookmark_path(@list), class:"btn btn-primary" %>
<%= link_to "all lists",  lists_path, class:"btn btn-primary" %>